{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"source": {
		"include": ["Components/CustomButton/",
					"Components/SpecsTab/",
					"Components/Slider/",
                    "Components/CustomPopover"],
		"exclude": [],
		"includePattern": ".+\\.js(doc)?$",
		"excludePattern": "(^|\\/|\\\\)_"
    },
	"plugins": ["plugins/markdown"],
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"useLongnameInNav": false,
		"showInheritedInNav": true
	},
	"opts": {
		"destination": "docs/",
		"encoding": "utf8",
		"recurse": true,
		"template": "docs/templates/minami"
	}
}
